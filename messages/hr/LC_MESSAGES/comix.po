msgid "First page"
msgstr "Prva stranica"

#: comix:11072
msgid "Previous page"
msgstr "Prijašnja stranica"

#: comix:11073
msgid "Next page"
msgstr "Sljedeća stranica"

#: comix:11074
msgid "Last page"
msgstr "Zadnja stranica"

#: comix:11075
msgid "Go to page..."
msgstr "Idi na stranicu..."

