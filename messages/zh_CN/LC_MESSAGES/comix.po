msgid "First page"
msgstr "第一页"

#: comix:11072
msgid "Previous page"
msgstr "前一页"

#: comix:11073
msgid "Next page"
msgstr "后一页"

#: comix:11074
msgid "Last page"
msgstr "最后一页"

#: comix:11075
msgid "Go to page..."
msgstr "跳到页面..."

